{
  "name": "floor12/yii2-module-pages",
  "description": "Yii2 module to organize pages with sitemap, breadcrumbs and many more.",
  "type": "yii2-extension",
  "minimum-stability": "stable",
  "keywords": [
    "floor12",
    "yii2 module",
    "yii",
    "yii2",
    "yii 2"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "floor12",
      "email": "primelifecoder@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.13",
    "yii2mod/yii2-enum": "*",
    "floor12/yii2-editmodal": "dev-master",
    "floor12/yii2-metamaster": "dev-master",
    "floor12/yii2-summernote": "dev-master",
    "floor12/yii2-text-counter-widget": "dev-main",
    "floor12/yii2-youtube-async-widget": "dev-master",
    "phpdocumentor/reflection-docblock": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "floor12\\pages\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "floor12\\pages\\tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-editmodal"
    },
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-summernote"
    },
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-metamaster"
    }
  ]
}
