{
  "name": "floor12/yii2-module-pages",
  "description": "Yii2 module to organize pages with sitemap, breadcrumbs and many more.",
  "type": "yii2-extension",
  "minimum-stability": "stable",
  "keywords": [
    "floor12",
    "yii2 module",
    "yii",
    "yii2",
    "yii 2"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "floor12",
      "email": "primelifecoder@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.13",
    "floor12/yii2-editmodal": "dev-master",
    "floor12/yii2-opengraph": "dev-master",
    "bower-asset/summernote": "@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
      "floor12\\pages\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "floor12\\files\\tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-editmodal"
    },
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-opengraph"
    },
    {
      "type": "git",
      "url": "https://github.com/floor12/yii2-module-files"
    }
  ]
}
